{
  "expo": {
    "name": "Stakkit",
    "slug": "stakkit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stakkit.app",
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.stakkit.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "scheme": "stakkit",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-web-browser",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
